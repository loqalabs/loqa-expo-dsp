{
  "name": "@loqalabs/loqa-expo-dsp",
  "version": "0.2.7",
  "description": "Production-grade Expo native module wrapping loqa-voice-dsp for audio DSP analysis (FFT, pitch detection, formants, HNR, H1-H2)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib",
    "ios",
    "android",
    "expo-module.config.json",
    "README.md",
    "docs/API.md",
    "docs/INTEGRATION_GUIDE.md",
    "LICENSE"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf lib",
    "lint": "eslint . --ext .ts,.tsx",
    "typecheck": "tsc --noEmit",
    "format": "prettier --write \"**/*.{ts,tsx,json,md}\"",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "verify:ios-symbols": "./scripts/verify-ios-symbols.sh",
    "verify:android-symbols": "./scripts/verify-android-symbols.sh",
    "verify:native-symbols": "npm run verify:ios-symbols && npm run verify:android-symbols",
    "validate:structure": "./scripts/validate-module-structure.sh",
    "prepare": "expo-module prepare",
    "prepublishOnly": "npm run build",
    "expo-module": "expo-module"
  },
  "keywords": [
    "audio",
    "dsp",
    "fft",
    "pitch",
    "formants",
    "spectrum",
    "hnr",
    "h1h2",
    "voice-analysis",
    "react-native",
    "expo"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/loqalabs/loqa-expo-dsp.git"
  },
  "bugs": {
    "url": "https://github.com/loqalabs/loqa-expo-dsp/issues"
  },
  "author": "Loqa Labs <33356887+annabarnes1138@users.noreply.github.com> (https://github.com/loqalabs)",
  "license": "MIT",
  "homepage": "https://github.com/loqalabs/loqa-expo-dsp#readme",
  "peerDependencies": {
    "expo": "^54.0.0",
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/react": "^19.0.0",
    "eslint": "^8.0.0",
    "eslint-config-expo": "^10.0.0",
    "eslint-config-prettier": "^10.1.8",
    "expo": "^54.0.18",
    "expo-module-scripts": "^5.0.7",
    "jest": "^30.2.0",
    "jest-expo": "^54.0.13",
    "prettier": "^3.0.0",
    "react": "^19.2.0",
    "react-native": "0.81.5",
    "ts-jest": "^29.4.5",
    "typescript": "^5.3.0"
  }
}
